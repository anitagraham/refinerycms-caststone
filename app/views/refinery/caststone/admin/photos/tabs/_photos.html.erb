<%
  content = content_tag(:div, class: 'wym_skin_refinery page_part', id:'page_photo_picker') do
    if Refinery::Caststone::Photo.all.count == 0
      concat content_tag(:p, t('.no_photos_yet'))
    else
      concat select :page, :photo_ids, Refinery::Caststone::Photo.all.map{ |p|
        [p.name, p.id,  ] }, {}, {:multiple=>true, :class=>:multiselect, :id=>:photo_multiselect}
    end
  end
%>
<%= content %>
