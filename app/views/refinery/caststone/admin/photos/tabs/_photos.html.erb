<%
  content = content_tag(:div, class: 'wym_skin_refinery page_part asdf', id:'page_photo_picker') do
    if @photos.nil?
      content += content_tag(:p, t('.no_photos_yet'))
    else
      content += select :page, :photo_ids,@photos.map{ |p| [p.name, p.id, { 'data-url' => p.thumbnail('250x')} ] }, {},    {:multiple=>true, :class=>:multiselect, :id=>:photo_multiselect}
    end
  end
  logger.debug "================== Page content: #{content}"
%>
<%= content %>
