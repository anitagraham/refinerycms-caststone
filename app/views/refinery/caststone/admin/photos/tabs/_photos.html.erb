<%
  content = content_tag(:div, class: 'wym_skin_refinery page_part', id:'page_photo_picker') do
    if @photos.nil?
      concat content_tag(:p, t('.no_photos_yet'))
    else
      concat select :page, :photo_ids, @photos.map{ |p| [p.name, p.id, { 'data-url' => p.thumbnail({geometry: '250x'})} ] }, {},    {:multiple=>true, :class=>:multiselect, :id=>:photo_multiselect}
    end
  end
%>
<%= content %>
